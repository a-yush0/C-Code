#include <iostream>

using namespace std;

// Function to calculate factorial
long long factorial(long long n) {
  long long result = 1;
  for (long long i = 1; i <= n; ++i) {
    result *= i;
  }
  return result;
}

int main() {
  long long n;
  cout << "Enter a number: ";
  cin >> n;

  cout << "The factorial of " << n << " is: " << factorial(n) << endl;

  return 0;
}
